<?php

/**
 * @file
 * Install file for the ai_drush_agents module.
 */

/**
 * Implements hook_uninstall().
 */
function ai_drush_agents_uninstall() {
  // Remove the agents.
  foreach ([
    'config_export_explainer',
    'drush_explain_agent',
  ] as $agent_id) {
    $config = \Drupal::configFactory()->getEditable('ai_agents.ai_agent.' . $agent_id);
    $config->delete();
  }
}
